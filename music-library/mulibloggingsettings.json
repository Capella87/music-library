{
    "Serilog": {
        "Using": [ "Serilog.Sinks.File" ],
        "MinimumLevel": {
            "Default": "Debug"
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": null,
                    "rollOnFileSizeLimit": false,
                    "fileSizeLimitBytes": 4194304,
                    "retainedFileCountLimit": 12,
                    "rollingInterval": "Year"
                }
            }
        ]
    }
}